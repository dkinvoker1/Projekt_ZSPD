@model IEnumerable<ZSPD.Domain.Models.EntityModels.Accounts.Student>

@{
    ViewBag.Title = "StudentsManage";
}
<link href="~/Content/bootstrap.min.css" rel="stylesheet" />
@Html.Partial("_HeaderNavBar", new ViewDataDictionary { { "name", "Zarządzanie studentami" } })


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.ActionLink("Szczegóły", "StudentDetails", new { id=item.Id }) |
            @Html.ActionLink("Usuń", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>

<div class="container centering-container">
    @Html.ActionLink("Wstecz", "Admin", "Home", null, new { @class = "own-btn btn-info btn" })
</div>

